.mainView
{
  background-color:white;
  width:100%;
  height:100%;
  display:flex;
  flex-direction:column;
  justify-content:flex-start;
  align-items:stretch;
}


.contentView
{
  width:100%;
  display:flex;
  flex-direction:column;
  justify-content:flex-start;
  align-items:flex-start;
  margin-top:30rpx;
}

.contentHeader
{
  font-size:20pt;
  font-weight:bold;
  margin-left:30rpx;
}

.contentCellView
{
  display:flex;
  width:100%;
  flex-direction:column;
  justify-content:flex-start;
  align-items:flex-start;
}

.contentImage
{
  margin-left:30rpx;
  margin-bottom:30rpx;
  width: 690rpx;
  height: 400rpx;
}


