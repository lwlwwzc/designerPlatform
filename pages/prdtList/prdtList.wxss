.btnBarView
{
    background-color:white;
    width:100%;
    height:50rpx;
    display:flex;
    flex-flow:row;
    justify-content:flex-start;
    align-items:center;
}

.tabView
{
    width:50%;
    display:flex;
    flex-flow:row;
    justify-content:center;
    align-items:center;
}

.tabBarText
{
    font-size:50rpx;
    color:black;
}

.tabBarText.active
{
    color:red;
}

.itemCell
{
    width:690rpx;
    margin-left:30rpx;
    margin-top:30rpx;
    background-color:white;
    height:360rpx;
}

.itemImage
{
    margin-left:30rpx;
    margin-top:30rpx;
    width:50%;
    height:300rpx;
}

.itemRightView
{
    margin-left:30rpx;
    margin-top:30rpx;
    width:50%;
}

.itemText
{
    font-size:30rpx;
    word-wrap:normal;
    word-break:break-all;
    margin-bottom:15rpx;
}